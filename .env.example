# 数据库配置
DATABASE_URL=mysql://username:password@localhost:3306/mcp_gateway

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=3000

# 日志级别
RUST_LOG=info

# 阿里云百炼向量模型配置
# 阿里云百炼 API Key
ALIYUN_BAILIAN_API_KEY=your_aliyun_bailian_api_key_here

# 阿里云百炼模型名称
ALIYUN_BAILIAN_MODEL=text-embedding-v4

# 阿里云百炼 API 端点
ALIYUN_BAILIAN_ENDPOINT=https://dashscope.aliyuncs.com/api/v1/services/embeddings/text-embedding/text-embedding

# 阿里云百炼工作空间 ID（可选）
ALIYUN_BAILIAN_WORKSPACE_ID=your_workspace_id_here

# 向量维度配置
EMBEDDING_DIMENSION=1536

# 向量模型类型 (simple | aliyun-bailian)
EMBEDDING_MODEL_TYPE=simple

# Prometheus 监控配置
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090

# CORS 配置
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# JWT 配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=24h

# Redis 配置（如果使用缓存）
REDIS_URL=redis://localhost:6379

# 文件上传配置
MAX_FILE_SIZE=10485760  # 10MB
UPLOAD_DIR=./uploads